{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@now/static-build",
        "config": { "distDir": "public" }
      }
    ],
    "scope": "the-sporkening",
    "alias": ["getsporked"],
    "routes": [
      {
        "src": "/_nuxt/.+",
        "headers": {
          "Cache-Control": "max-age=31557600"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  